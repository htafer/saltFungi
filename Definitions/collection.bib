@article{SONG:04,
abstract = {ARGONAUTE PROTEINS AND SMALL INTERFERING RNAS (SIRNAS) ARE THE KNOWN
SIGNATURE COMPONENTS OF THE RNA INTERFERENCE EFFECTOR COMPLEX RNA-INDUCED
SILENCING COMPLEX (RISC). HOWEVER, THE IDENTITY OF {\#}SLICER{\#}, THE
ENZYME THAT CLEAVES THE MESSENGER RNA (MRNA) AS DIRECTED BY THE SIRNA,
