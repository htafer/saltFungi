journal = {Antonie van Leeuwenhoek, Int. J. Gen. Mol. Microbiol.},
keywords = {Debaryomyces,Genotypic identification,Geotrichum,Issatchenkia,Kluyveromyces,Phenotypic identification,Physiology,RAPD-PCR,Saccharomyces,Yarrowia},
pages = {267--283},
pmid = {10510714},
title = {{Phenotypic and genotypic identification of yeasts from cheese}},
volume = {75},
