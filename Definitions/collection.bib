issn = {1932-6203},
journal = {PLoS One},
keywords = {Applied microbiology,Bacterial and foodborne illness,Bacterial diseases,Bacterial pathogens,Bacterial physiology,Bacteriology,Biology,Gastroenterology and hepatology,Genome expression analysis,Genomics,Gram positive,Infectious diseases,Medicine,Microbial control,Microbial ecology,Microbial metabolism,Microbial pathogens,Microbial physiology,Microbiology,Research Article},
month = {jan},
number = {2},
pages = {e89958},
pmid = {24587151},
publisher = {Public Library of Science},
title = {{Transcriptomic analysis of (group I) Clostridium botulinum ATCC 3502 cold shock response.}},

