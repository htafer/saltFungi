month = {jun},
number = {6},
pages = {376--383},
pmid = {19467477},
title = {{Cerebral phaeohyphomycosis—a cure at what lengths?}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19467477},
volume = {9},
year = {2009}
