journal = {Scand. J. Plast. Reconstr. Surg. Hand Surg.},
pages = {107--112},
pmid = {7521538},
title = {{Insulin like growth factor-1 and -2 and their role in the re-epithelialisation of wounds; interactions with insulin like growth factor binding protein type 1.}},
