journal = {BMC Microbiol.},
keywords = {Antifungal Agents,Antifungal Agents: pharmacology,Cold Temperature,Dose-Response Relationship,Drug,Drug Resistance,Electron,Electron Spin Resonance Spectroscopy,Electron Spin Resonance Spectroscopy: methods,Enzyme Stability,Enzyme Stability: genetics,Exophiala,Exophiala: drug effects,Exophiala: enzymology,Exophiala: ultrastructure,Fluorescence,Fluorescence: methods,Fungal,Fungal: genetics,Hot Temperature,Melanins,Melanins: biosynthesis,Miconazole,Miconazole: pharmacology,Microscopy,Mutation,Mutation: genetics,Naphthols,Polyketide Synthases,Polyketide Synthases: chemistry,Polyketide Synthases: genetics,Polyketide Synthases: metabolism,Scanning,Scanning: methods,Transmission,Transmission: methods},
month = {jan},
