keywords = {Amino Acid Sequence,Biological Products,Biological Products: chemistry,Biological Products: metabolism,Echinocandins,Echinocandins: chemistry,Echinocandins: metabolism,Fungal,Fungal Proteins,Fungal Proteins: chemistry,Fungal Proteins: metabolism,Fungi,Fungi: chemistry,Fungi: enzymology,Fungi: genetics,Fungi: metabolism,Genome,Molecular Sequence Data,Molecular Structure,Peptide Synthases,Peptide Synthases: metabolism,Polyketides,Polyketides: chemistry,Polyketides: metabolism},
month = {oct},
