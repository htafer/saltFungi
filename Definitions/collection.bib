journal = {BMC Microbiol.},
keywords = {Ammonium Sulfate,Ammonium Sulfate: metabolism,Animals,Candida albicans,Candida albicans: genetics,Candida albicans: metabolism,Candida albicans: pathogenicity,Cell Adhesion,DNA, Fungal,DNA, Fungal: genetics,Gene Expression Profiling,Gene Expression Profiling: methods,Genes, Fungal,Genome, Fungal,Genomics,Genomics: methods,Humans,Hydrogen-Ion Concentration,Mice,NIH 3T3 Cells,Oligonucleotide Array Sequence Analysis,Open Reading Frames,Phenotype,RNA, Fungal,RNA, Fungal: genetics,Transcription, Genetic,Virulence},
