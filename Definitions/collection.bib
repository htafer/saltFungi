pages = {4973--4980},
pmid = {12930946},
title = {{Developmental defects by antisense-mediated inactivation of micro-{\{}RNA{\}}s 2 and 13 in {\{}D{\}}rosophila and the identification of putative target genes.}},
volume = {31},
year = {2003}
}
@article{SCHATTNER:04,
abstract = {ONE OF THE LARGEST FAMILIES OF SMALL RNAS IN EUKARYOTES IS THE H/ACA
SMALL NUCLEOLAR RNAS (SNORNAS), MOST OF WHICH GUIDE RNA PSEUDOURIDINE
FORMATION. SO FAR, AN EFFECTIVE COMPUTATIONAL METHOD SPECIFICALLY
FOR IDENTIFYING H/ACA SNORNA GENE SEQUENCES HAS NOT BEEN ESTABLISHED.
WE HAVE DEVELOPED SNOGPS, A PROGRAM FOR COMPUTATIONALLY SCREENING
GENOMIC SEQUENCES FOR H/ACA GUIDE SNORNAS. THE PROGRAM IMPLEMENTS
A DETERMINISTIC SCREENING ALGORITHM COMBINED WITH A PROBABILISTIC
MODEL TO SCORE GENE CANDIDATES. WE REPORT HERE THE RESULTS OF TESTING
SNOGPS ON THE BUDDING YEAST SACCHAROMYCES CEREVISIAE. SIX CANDIDATE
SNORNAS WERE VERIFIED AS NOVEL RNA TRANSCRIPTS, AND FIVE OF THESE
WERE VERIFIED AS GUIDES FOR PSEUDOURIDINE FORMATION AT SPECIFIC SITES
IN RIBOSOMAL RNA. WE ALSO PREDICTED 14 NEW BASE-PAIRINGS BETWEEN
SNORNAS AND KNOWN PSEUDOURIDINE SITES IN S.CEREVISIAE RRNA, 12 OF
WHICH WERE VERIFIED BY GENE DISRUPTION AND LOSS OF THE COGNATE PSEUDOURIDINE
SITE. OUR FINDINGS INCLUDE THE FIRST PREDICTION AND VERIFICATION
OF SNORNAS THAT GUIDE PSEUDOURIDINE MODIFICATION AT MORE THAN TWO
SITES. WITH THIS WORK, 41 OF THE 44 KNOWN PSEUDOURIDINE MODIFICATIONS
IN S.CEREVISIAE RRNA HAVE BEEN LINKED WITH A VERIFIED SNORNA, PROVIDING
THE MOST COMPLETE ACCOUNTING OF THE H/ACA SNORNAS THAT GUIDE PSEUDOURIDYLATION
IN ANY SPECIES.},
author = {SCHATTNER, PETER and DECATUR, WAYNE A and DAVIS, CARRIE A and ARES, MANUEL and FOURNIER, MAURILLE J and LOWE, TODD M},
doi = {10.1093/NAR/GKH768},
institution = {DEPARTMENT OF BIOMOLECULAR ENGINEERING, UNIVERSITY OF CALIFORNIA, SANTA CRUZ, 1156 HIGH STREET, SANTA CRUZ, CA 95064, USA.},
journal = {NUCLEIC ACIDS RES},
keywords = {ALGORITHMS; BASE SEQUENCE; COMPUTATIONAL BIOLOGY;,FUNGAL; GENOMICS; MOLECULAR SEQUENCE DATA; PHYLOG,FUNGAL; RNA,GUIDE; RNA,RIBOSOMAL; RNA,SMALL NUCLEOLAR; SACCHAROMYCES CEREVISIAE; SOFTWA},
number = {14},
pages = {4281--4296},
pmid = {15306656},
title = {{GENOME-WIDE SEARCHING FOR PSEUDOURIDYLATION GUIDE SNORNAS: ANALYSIS OF THE SACCHAROMYCES CEREVISIAE GENOME.}},

