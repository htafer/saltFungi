month = {jun},
number = {1},
pages = {45--49},
pmid = {18581978},
title = {{A study of the growth of Pseudallescheria boydii isolates from sewage sludge and clinical sources on tributyrin, rapeseed oil, biodiesel oil and diesel oil.}},

