journal = {Nucleic Acids Res.},
keywords = {Algorithms,Alternative Splicing,Arabidopsis,Arabidopsis: genetics,Arabidopsis: metabolism,Complementary,Complementary: analysis,DNA,Expressed Sequence Tags,Genetic,Genome,Introns,Plant,Plant Proteins,Plant Proteins: genetics,Plant: analysis,Plant: chemistry,RNA,Sequence Alignment,Sequence Alignment: methods,Software,Transcription,Untranslated Regions},
month = {oct},
