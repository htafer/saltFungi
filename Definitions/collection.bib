journal = {NUCLEIC ACIDS RES},
month = {apr},
number = {5},
pages = {1602--1615},
pmid = {19151082},
title = {{NON-CODING RNA ANNOTATION OF THE GENOME OF TRICHOPLAX ADHAERENS}},

volume = {37},
year = {2009}
}
