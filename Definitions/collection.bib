@article{Carrington:03,
abstract = {Small RNAs, including microRNAs (miRNAs) and short interfering RNAs
(siRNAs), are key components of an evolutionarily conserved system
of RNA-based gene regulation in eukaryotes. They are involved in
many molecular interactions, including defense against viruses and
regulation of gene expression during development. miRNAs interfere
