volume = {309},
year = {2005}
}
@article{Macrae:06,
abstract = {The specialized ribonuclease Dicer initiates RNA interference by cleaving
double-stranded RNA (dsRNA) substrates into small fragments about
25 nucleotides in length. In the crystal structure of an intact Dicer
enzyme, the PAZ domain, a module that binds the end of dsRNA, is
separated from the two catalytic ribonuclease III (RNase III) domains
by a flat, positively charged surface. The 65 angstrom distance between
