journal = {J. Theor. Biol.},
keywords = {Computational Biology,Computational Biology: methods,Enterobacteriaceae,Enterobacteriaceae: genetics,Escherichia coli,Escherichia coli: genetics,Gene Expression Regulation, Bacterial,Gene Expression Regulation, Bacterial: genetics,Neural Networks (Computer),Promoter Regions, Genetic,Promoter Regions, Genetic: genetics,Sigma Factor,Sigma Factor: genetics},
month = {oct},
