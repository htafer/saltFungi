pmid = {19535462},
title = {{Efficient oligonucleotide-mediated degradation of nuclear noncoding RNAs in mammalian cultured cells.}},

volume = {15},
year = {2009}
}
@article{Martin2012,
abstract = {A majority of SNPs (single nucleotide polymorphisms) map to noncoding and intergenic regions of the genome. Noncoding SNPs are often identified in genome-wide association studies (GWAS) as strongly associated with human disease. Two such disease-associated SNPs in the 5' UTR of the human FTL (Ferritin Light Chain) gene are predicted to alter the ensemble of structures adopted by the mRNA. High-accuracy single nucleotide resolution chemical mapping reveals that these SNPs result in substantial changes in the structural ensemble in agreement with the computational prediction. Furthermore six rescue mutations are correctly predicted to restore the mRNA to its wild-type ensemble. Our data confirm that the FTL 5' UTR is a "RiboSNitch," an RNA that changes structure if a particular disease-associated SNP is present. The structural change observed is analogous to that of a bacterial Riboswitch in that it likely regulates translation. These data further suggest that specific pairs of SNPs in high linkage disequilibrium (LD) will form RNA structure-stabilizing haplotypes (SSHs). We identified 484 SNP pairs that form SSHs in UTRs of the human genome, and in eight of the 10 SSH-containing transcripts, SNP pairs stabilize RNA protein binding sites. The ubiquitous nature of SSHs in the transcriptome suggests that certain haplotypes are conserved to avoid RiboSNitch formation.},
author = {Martin, Joshua S and Halvorsen, Matthew and Davis-Neulander, Lauren and Ritz, Justin and Gopinath, Chetna and Beauregard, Arthur and Laederach, Alain},
doi = {10.1261/rna.029900.111},
file = {:home/htafer/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Martin et al. - 2012 - Structural effects of linkage disequilibrium on the transcriptome.pdf:pdf},
issn = {1469-9001},
journal = {RNA},
keywords = {5' Untranslated Regions,5' Untranslated Regions: genetics,Apoferritins,Apoferritins: genetics,Genome, Human,Genome, Human: genetics,Haplotypes,Humans,Linkage Disequilibrium,Mutation,Nucleic Acid Conformation,Polymorphism, Single Nucleotide,RNA,RNA-Binding Proteins,RNA-Binding Proteins: genetics,RNA-Binding Proteins: metabolism,RNA: chemistry,RNA: genetics,Transcriptome,Transcriptome: genetics},
month = {jan},
number = {1},
pages = {77--87},
pmid = {22109839},
title = {{Structural effects of linkage disequilibrium on the transcriptome.}},

volume = {18},
year = {2012}
}
@article{Ero2008,
abstract = {In ribosomal RNA, modified nucleosides are found in functionally important regions, but their function is obscure. Stem-loop 69 of Escherichia coli 23S rRNA contains three modified nucleosides: pseudouridines at positions 1911 and 1917, and N3 methyl-pseudouridine (m(3)Psi) at position 1915. The gene for pseudouridine methyltransferase was previously not known. We identified E. coli protein YbeA as the methyltransferase methylating Psi1915 in 23S rRNA. The E. coli ybeA gene deletion strain lacks the N3 methylation at position 1915 of 23S rRNA as revealed by primer extension and nucleoside analysis by HPLC. Methylation at position 1915 is restored in the ybeA deletion strain when recombinant YbeA protein is expressed from a plasmid. In addition, we show that purified YbeA protein is able to methylate pseudouridine in vitro using 70S ribosomes but not 50S subunits from the ybeA deletion strain as substrate. Pseudouridine is the preferred substrate as revealed by the inability of YbeA to methylate uridine at position 1915. This shows that YbeA is acting at the final stage during ribosome assembly, probably during translation initiation. Hereby, we propose to rename the YbeA protein to RlmH according to uniform nomenclature of RNA methyltransferases. RlmH belongs to the SPOUT superfamily of methyltransferases. RlmH was found to be well conserved in bacteria, and the gene is present in plant and in several archaeal genomes. RlmH is the first pseudouridine specific methyltransferase identified so far and is likely to be the only one existing in bacteria, as m(3)Psi1915 is the only methylated pseudouridine in bacteria described to date.},
author = {Ero, Rya and Peil, Lauri and Liiv, Aivar and Remme, Jaanus},
doi = {10.1261/rna.1186608},
file = {:home/htafer/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ero et al. - 2008 - Identification of pseudouridine methyltransferase in Escherichia coli.pdf:pdf},
issn = {1469-9001},
journal = {RNA},
keywords = {Amino Acid Sequence,Conserved Sequence,Escherichia coli,Escherichia coli Proteins,Escherichia coli Proteins: genetics,Escherichia coli Proteins: metabolism,Escherichia coli: enzymology,Escherichia coli: genetics,Methylation,Methyltransferases,Methyltransferases: genetics,Methyltransferases: metabolism,Molecular Sequence Data,Pseudouridine,Pseudouridine: metabolism,RNA, Ribosomal, 23S,RNA, Ribosomal, 23S: metabolism},
month = {oct},
number = {10},
pages = {2223--33},
pmid = {18755836},
title = {{Identification of pseudouridine methyltransferase in Escherichia coli.}},

volume = {14},
year = {2008}
}
@article{Ambros:03,
abstract = {MicroRNAs (miRNAs) are small noncoding RNA gene products about 22
nt long that are processed by Dicer from precursors with a characteristic
hairpin secondary structure. Guidelines are presented for the identification
and annotation of new miRNAs from diverse organisms, particularly
so that miRNAs can be reliably distinguished from other RNAs such
as small interfering RNAs. We describe specific criteria for the
experimental verification of miRNAs, and conventions for naming miRNAs
and miRNA genes. Finally, an online clearinghouse for miRNA gene
name assignments is provided by the Rfam database of RNA families.},
author = {Ambros, V and Bartel, B and Bartel, D P and Burge, C B and Carrington, J C and Chen, X and Dreyfuss, G and Eddy, S R and Griffiths-Jones, S and Marshall, M and Matzke, M and Ruvkun, G and Tuschl, T},
