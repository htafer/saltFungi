journal = {FEMS Yeast Res.},
keywords = {Animals,Candida,Candida: classification,Candida: genetics,Cluster Analysis,DNA Fingerprinting,DNA Fingerprinting: methods,DNA, Fungal,DNA, Fungal: genetics,DNA, Ribosomal Spacer,DNA, Ribosomal Spacer: genetics,Food Microbiology,Genotype,Humans,Mycological Typing Techniques,Phylogeny,Plants,Plants: microbiology,Polymorphism, Restriction Fragment Length,Saccharomycetales,Saccharomycetales: classification,Saccharomycetales: genetics},
month = {jun},
number = {4},
pages = {641--62},
pmid = {19385997},
title = {{Differentiation of Debaryomyces hansenii and Candida famata by rRNA gene intergenic spacer fingerprinting and reassessment of phylogenetic relationships among D. hansenii, C. famata, D. fabryi, C. flareri (=D. subglobosus) and D. prosopidis: description o}},

