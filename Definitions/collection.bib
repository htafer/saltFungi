title = {{Efflux in fungi: la pi{\{}{\`{e}}{\}}ce de r{\{}{\'{e}}{\}}sistance.}},

volume = {5},
year = {2009}
}
@article{Sun2010,
abstract = {Candida albicans Ssa1 and Ssa2 are members of the HSP70 family of heat shock proteins that are expressed on the cell surface and function as receptors for antimicrobial peptides such as histatins. We investigated the role of Ssa1 and Ssa2 in mediating pathogenic host cell interactions and virulence. A C. albicans ssa1{\{}{\$}{\}}\backslashDelta{\{}\backslash{\$}{\}}/{\{}{\$}{\}}\backslashDelta{\{}\backslash{\$}{\}}mutant had attenuated virulence in murine models of disseminated and oropharyngeal candidiasis, whereas an ssa2{\{}{\$}{\}}\backslashDelta{\{}\backslash{\$}{\}}/{\{}{\$}{\}}\backslashDelta{\{}\backslash{\$}{\}}mutant did not. In vitro studies revealed that the ssa1{\{}{\$}{\}}\backslashDelta{\{}\backslash{\$}{\}}/{\{}{\$}{\}}\backslashDelta{\{}\backslash{\$}{\}}mutant caused markedly less damage to endothelial cells and oral epithelial cell lines. Also, the ssa1{\{}{\$}{\}}\backslashDelta{\{}\backslash{\$}{\}}/{\{}{\$}{\}}\backslashDelta{\{}\backslash{\$}{\}}mutant had defective binding to endothelial cell N-cadherin and epithelial cell E-cadherin, receptors that mediate host cell endocytosis of C. albicans. As a result, this mutant had impaired capacity to induce its own endocytosis by endothelial cells and oral epithelial cells. Latex beads coated with recombinant Ssa1 were avidly endocytosed by both endothelial cells and oral epithelial cells, demonstrating that Ssa1 is sufficient to induce host cell endocytosis. These results indicate that Ssa1 is a novel invasin that binds to host cell cadherins, induces host cell endocytosis, and is critical for C. albicans to cause maximal damage to host cells and induce disseminated and oropharyngeal disease.},
author = {Sun, Jianing N and Solis, Norma V and Phan, Quynh T and Bajwa, Jashanjot S and Kashleva, Helena and Thompson, Angela and Liu, Yaoping and Dongari-Bagtzoglou, Anna and Edgerton, Mira and Filler, Scott G},
doi = {10.1371/journal.ppat.1001181},
issn = {1553-7374},
journal = {PLoS Pathog.},
month = {jan},
number = {11},
pages = {e1001181},
pmid = {21085601},
title = {{Host cell invasion and virulence mediated by Candida albicans Ssa1.}},
volume = {6},
year = {2010}
}
