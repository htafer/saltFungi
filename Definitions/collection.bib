@article{Storz:04,
abstract = {Recent studies have led to the identification of more than 50 small
regulatory RNAs in Escherichia coli. Only a subset of these RNAs
has been characterized. However, it is clear that many of the RNAs,
such as the MicF, OxyS, DsrA, Spot42 and RyhB RNAs, act by basepairing
to activate or repress translation or to destabilize mRNAs. Basepairing
