journal = {J. Mol. Evol.},
keywords = {Ascomycota,Ascomycota: metabolism,Basidiomycota,Basidiomycota: metabolism,Cell Lineage,Endoribonucleases,Endoribonucleases: genetics,Endoribonucleases: metabolism,Evolution, Molecular,Genes, Fungal,Genetic Variation,Phylogeny,RNA Interference,RNA-Directed DNA Polymerase,RNA-Directed DNA Polymerase: genetics,RNA-Directed DNA Polymerase: metabolism,Signal Transduction},
month = {jul},
