title = {{Argonaute2 is the catalytic engine of mammalian RNAi}},

volume = {305},
year = {2004}
}
@article{Storz:02,
abstract = {Noncoding RNAs (ncRNAs) have been found to have roles in a great
variety of processes, including transcriptional regulation, chromosome
replication, RNA processing and modification, messenger RNA stability
and translation, and even protein degradation and translocation.
Recent studies indicate that ncRNAs are far more abundant and important
than initially imagined. These findings raise several fundamental
questions: How many ncRNAs are encoded by a genome? Given the absence
of a diagnostic open reading frame, how can these genes be identified?
How can all the functions of ncRNAs be elucidated?},
author = {Storz, G},
