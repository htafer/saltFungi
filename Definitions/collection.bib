@article{Walsh2004,
abstract = {The emergence of less common but medically important fungal pathogens contributes to the rate of morbidity and mortality, especially in the increasingly expanding population of immunocompromised patients. These pathogens include septate filamentous fungi (e.g., Fusarium spp., Scedosporium spp., Trichoderma spp.), nonseptate Zygomycetes, the endemic dimorphic pathogen Penicillium marneffei, and non-Cryptococcus, non-Candida pathogenic yeast (e.g., Trichosporon spp.). The medical community is thus called upon to acquire an understanding of the microbiology, epidemiology and pathogenesis of these previously uncommon pathogens in order to become familiar with the options for prevention and treatment.},
author = {Walsh, T J and Groll, A and Hiemenz, J and Fleming, R and Roilides, E and Anaissie, E},
issn = {1198-743X},
