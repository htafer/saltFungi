title = {{Polyadenylation linked to transcription termination directs the processing of snoRNA precursors in yeast.}},

volume = {32},
year = {2008}
}
@article{Lemay2010,
abstract = {Poly(A)-binding proteins (PABPs) are important to eukaryotic gene expression. In the nucleus, the PABP PABPN1 is thought to function in polyadenylation of pre-mRNAs. Deletion of fission yeast pab2, the homolog of mammalian PABPN1, results in transcripts with markedly longer poly(A) tails, but the nature of the hyperadenylated transcripts and the mechanism that leads to RNA hyperadenylation remain unclear. Here we report that Pab2 functions in the synthesis of noncoding RNAs, contrary to the notion that PABPs function exclusively on protein-coding mRNAs. Accordingly, the absence of Pab2 leads to the accumulation of polyadenylated small nucleolar RNAs (snoRNAs). Our findings suggest that Pab2 promotes poly(A) tail trimming from pre-snoRNAs by recruiting the nuclear exosome. This work unveils a function for the nuclear PABP in snoRNA synthesis and provides insights into exosome recruitment to polyadenylated RNAs.},
author = {Lemay, Jean-Fran{\c{c}}ois and D'Amours, Annie and Lemieux, Caroline and Lackner, Daniel H and St-Sauveur, Val{\'{e}}rie G and B{\"{a}}hler, J{\"{u}}rg and Bachand, Fran{\c{c}}ois},
