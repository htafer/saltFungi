journal = {BMC Bioinformatics},
keywords = {Algorithms,Base Sequence,Genome, Bacterial,Genome, Bacterial: genetics,Molecular Sequence Data,Prokaryotic Initiation Factors,Prokaryotic Initiation Factors: genetics,Promoter Regions, Genetic,Promoter Regions, Genetic: genetics,Sequence Analysis,Sequence Analysis: methods,Transcription, Genetic,Transcription, Genetic: genetics,promoter search},
mendeley-tags = {promoter search},
pages = {423},
