doi = {10.1111/j.1365-2958.2007.05733.x},
institution = {D�partement de Biochimie, Groupe ARN, Universit� de Sherbrooke, Sherbrooke, Qu�bec, Canada.},
journal = {MOL MICROBIOL},
keywords = {5' UNTRANSLATED REGIONS,5' Untranslated Regions,BACTERIAL,BIOLOGICAL,Bacterial,Biological,MEMBRANE TRANSPORT PROTEI,MESSENGER,Membrane Transport Protei,Messenger,PROTEIN BIOSYNTHESIS,Protein Biosynthesis,RNA,SHIKIMIC ACID,SIDEROPHORES,Shikimic Acid,Siderophores},
month = {jun},
