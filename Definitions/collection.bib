journal = {Nucleic Acids Res.},
keywords = {Chromosome Mapping,Databases, Nucleic Acid,Databases, Nucleic Acid: statistics {\&} numerical da,Gene Expression Regulation, Bacterial,Genome, Bacterial,RNA, Bacterial,RNA, Bacterial: chemistry,RNA, Bacterial: genetics,RNA, Bacterial: metabolism,RNA, Untranslated,RNA, Untranslated: chemistry,RNA, Untranslated: genetics,RNA, Untranslated: metabolism,User-Computer Interface},
