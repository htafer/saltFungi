isbn = {0270-7306 (Print)$\backslash$r0270-7306 (Linking)},
issn = {0270-7306},
journal = {Mol. Cell. Biol.},
keywords = {Amino Acid Sequence,Base Sequence,Biological Transport,Cation Transport Proteins,Cloning,Cobalt,Cobalt: metabolism,DNA,Drug Resistance,Fungal,Fungal Proteins,Fungal Proteins: analysis,Fungal Proteins: genetics,Genes,Immunoblotting,Intracellular Membranes,Intracellular Membranes: chemistry,Membrane Transport Proteins,Microbial,Microbial: genetics,Mitochondria,Mitochondria: chemistry,Molecular,Molecular Sequence Data,Nucleic Acid,Phenotype,Restriction Mapping,Saccharomyces cerevisiae,Saccharomyces cerevisiae Proteins,Saccharomyces cerevisiae: genetics,Saccharomyces cerevisiae: growth {\&} development,Saccharomyces cerevisiae: metabolism,Sequence Homology},
number = {9},
