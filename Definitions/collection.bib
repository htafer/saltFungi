@article{Dujon2004,
abstract = {Identifying the mechanisms of eukaryotic genome evolution by comparative genomics is often complicated by the multiplicity of events that have taken place throughout the history of individual lineages, leaving only distorted and superimposed traces in the genome of each living organism. The hemiascomycete yeasts, with their compact genomes, similar lifestyle and distinct sexual and physiological properties, provide a unique opportunity to explore such mechanisms. We present here the complete, assembled genome sequences of four yeast species, selected to represent a broad evolutionary range within a single eukaryotic phylum, that after analysis proved to be molecularly as diverse as the entire phylum of chordates. A total of approximately 24,200 novel genes were identified, the translation products of which were classified together with Saccharomyces cerevisiae proteins into about 4,700 families, forming the basis for interspecific comparisons. Analysis of chromosome maps and genome redundancies reveal that the different yeast lineages have evolved through a marked interplay between several distinct molecular mechanisms, including tandem gene repeat formation, segmental duplication, a massive genome duplication and extensive gene loss.},
author = {Dujon, Bernard and Sherman, David and Fischer, Gilles and Durrens, Pascal and Casaregola, Serge and Lafontaine, Ingrid and {De Montigny}, Jacky and Marck, Christian and Neuv{\'{e}}glise, C{\'{e}}cile and Talla, Emmanuel and Goffard, Nicolas and Frangeul, Lionel and Aigle, Michel and Anthouard, V{\'{e}}ronique and Babour, Anna and Barbe, Val{\'{e}}rie and Barnay, St{\'{e}}phanie and Blanchin, Sylvie and Beckerich, Jean-Marie and Beyne, Emmanuelle and Bleykasten, Claudine and Boisram{\'{e}}, Anita and Boyer, Jeanne and Cattolico, Laurence and Confanioleri, Fabrice and {De Daruvar}, Antoine and Despons, Laurence and Fabre, Emmanuelle and Fairhead, C{\'{e}}cile and Ferry-Dumazet, H{\'{e}}l{\`{e}}ne and Groppi, Alexis and Hantraye, Florence and Hennequin, Christophe and Jauniaux, Nicolas and Joyet, Philippe and Kachouri, Rym and Kerrest, Alix and Koszul, Romain and Lemaire, Marc and Lesur, Isabelle and Ma, Laurence and Muller, H{\'{e}}lo{\"{i}}se and Nicaud, Jean-Marc and Nikolski, Macha and Oztas, Sophie and Ozier-Kalogeropoulos, Odile and Pellenz, Stefan and Potier, Serge and Richard, Guy-Franck and Straub, Marie-Laure and Suleau, Audrey and Swennen, Dominique and Tekaia, Fredj and W{\'{e}}solowski-Louvel, Micheline and Westhof, Eric and Wirth, B{\'{e}}n{\'{e}}dicte and Zeniou-Meyer, Maria and Zivanovic, Ivan and Bolotin-Fukuhara, Monique and Thierry, Agn{\`{e}}s and Bouchier, Christiane and Caudron, Bernard and Scarpelli, Claude and Gaillardin, Claude and Weissenbach, Jean and Wincker, Patrick and Souciet, Jean-Luc},
