journal = {Infection and Immunity},
number = {12},
pages = {6619--6630},
pmid = {10569783},
title = {{WdChs4p, a homolog of chitin synthase 3 in Saccharomyces cerevisiae, alone cannot support growth of Wangiella (Exophiala) dermatitidis at the temperature of infection}},
volume = {67},
year = {1999}
}


