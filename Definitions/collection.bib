journal = {J. Allergy Clin. Immunol.},
keywords = {Animals,Atopic,Atopic: immunology,Atopic: microbiology,Atopic: therapy,Bacteria,Bacteria: immunology,Bacteria: metabolism,Cathelicidins,Cathelicidins: immunology,Cathelicidins: metabolism,Cholecalciferol,Cholecalciferol: metabolism,Dermatitis,Humans,Psoriasis,Psoriasis: immunology,Psoriasis: microbiology,Psoriasis: therapy,Rosacea,Rosacea: immunology,Rosacea: microbiology,Rosacea: therapy,Skin,Skin Diseases,Skin Diseases: immunology,Skin Diseases: metabolism,Skin Diseases: therapy,Skin: immunology,Skin: metabolism,Skin: microbiology},
month = {aug},
