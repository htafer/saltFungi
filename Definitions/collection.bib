title = {{Morphological and Molecular Characterization of a Rock Inhabiting and Rock Decaying Dematiaceous Fungus Isolated from Antique Monuments of Delos (Cyclades, Greece) and Chersonesus (Crimea, Ukraine)}},
volume = {20},
year = {1997}
}
