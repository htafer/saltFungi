journal = {Med. Mycol.},
keywords = {DNA, Fungal,DNA, Fungal: chemistry,DNA, Fungal: genetics,DNA, Ribosomal Spacer,DNA, Ribosomal Spacer: chemistry,DNA, Ribosomal Spacer: genetics,Environmental Microbiology,Household Articles,Humans,Molecular Sequence Data,Sequence Analysis, DNA,Turkey,Yeasts,Yeasts: classification,Yeasts: genetics,Yeasts: isolation {\&} purification},
month = {jul},
number = {5},
pages = {493--8},
pmid = {23167707},
title = {{Dishwashers are a major source of human opportunistic yeast-like fungi in indoor environments in Mersin, Turkey.}},

