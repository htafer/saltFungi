@ARTICLE{Tamura2013,
  title    = "{MEGA6}: Molecular Evolutionary Genetics Analysis version 6.0",
  author   = "Tamura, Koichiro and Stecher, Glen and Peterson, Daniel and
              Filipski, Alan and Kumar, Sudhir",
  abstract = "We announce the release of an advanced version of the Molecular
              Evolutionary Genetics Analysis (MEGA) software, which currently
